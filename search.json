---
layout: null
---

[
  {% for post in site.posts %}
    {
      "url"      : "{{ post.url | relative_url }}",
      "thumbnail": "{{ post.thumbnail }}",
      "title"    : "{{ post.title }}",
      "date"     : "{{ post.date | date: '%B %d, %Y' }}",
      "score"    : "{{ post.review.overall }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]